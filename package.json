{
  "name": "front-end-social",
  "version": "2.0.0",
  "description": "An 11ty site about the mastodon server.",
  "main": "index.html",
  "scripts": {
    "watch:sass": "sass --watch src/sass:public/css",
    "watch:eleventy": "eleventy --serve",
    "build:sass": "sass src/sass:public/css",
    "build:eleventy": "eleventy",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:sass build:eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mirisuzanne/front-end-social.git"
  },
  "author": "Miriam Suzanne <miriam@oddbird.net>",
  "contributors": [
    "Estelle Weyl <estelle@openwebdocs.org>",
    "Keith Grant <keithjgrant@gmail.com>"
  ],
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.54.9"
  }
}
